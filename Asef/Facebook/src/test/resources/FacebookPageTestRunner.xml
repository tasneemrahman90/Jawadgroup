<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Facebook" parallel="tests">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="userName" value="asefrahman"/>
    <parameter name="accessKey" value=""/>
    <parameter name="os" value="Windows 10"/>
    <parameter name="browserName" value="firefox"/>
    <parameter name="browserVersion" value="48.0"/>
    <parameter name="url" value="http://www.facebook.com"/>

    <test name = "Facebook login">
        <classes>
            <class name="login.page.login"/>
        </classes>
    </test>
    <test name = "Facebook signup">
        <classes>
            <class name="login.page.signup"/>
        </classes>
    </test>


</suite>